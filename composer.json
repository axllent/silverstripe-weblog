{
    "name": "axllent/silverstripe-weblog",
    "description": "A blog module for Silverstripe",
    "type": "silverstripe-vendormodule",
    "homepage": "https://github.com/axllent/silverstripe-weblog",
    "keywords": [
        "silverstripe",
        "blog",
        "news"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ralph Slooten",
            "homepage": "http://www.axllent.org/"
        }
    ],
    "support": {
        "issues": "https://github.com/axllent/silverstripe-weblog/issues"
    },
    "require": {
        "silverstripe/vendor-plugin": "^3.0",
        "silverstripe/cms": "^6.0",
        "silverstripe/assets": "^3.0",
        "silverstripe/asset-admin": "^3.0",
        "silverstripe/lumberjack": "^4.0"
    },
    "extra": {
        "expose": [
            "css",
            "icons"
        ]
    },
    "autoload": {
        "psr-4": {
            "Axllent\\Weblog\\": "src/"
        }
    },
    "suggest": {
        "axllent/silverstripe-weblog-categories": "optional: add categories to your blog posts"
    },
    "conflict": {
        "silverstripe/blog": "*"
    }
}
